{
  "name": "@keplr-ewallet/ewallet-sdk-core",
  "version": "0.0.3",
  "packageManager": "yarn@4.7.0",
  "type": "module",
  "scripts": {
    "clean": "del-cli dist",
    "build": "tsc && tsc-alias"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "keplr",
    "ewallet",
    "cosmos",
    "blockchain",
    "wallet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chainapsis/keplr-ewallet.git",
    "directory": "sdk/ewallet_sdk_core"
  },
  "devDependencies": {
    "@types/jest": "*",
    "@types/node": "*",
    "del-cli": "*",
    "jest": "*",
    "ts-jest": "*",
    "tsc-alias": "*",
    "typescript": "*"
  }
}
